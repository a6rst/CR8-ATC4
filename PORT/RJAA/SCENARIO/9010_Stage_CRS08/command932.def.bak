  {$NODE_NAMEPOINTCOUNT_GWY > 0} "[AP_RMP_TAXI_SPOT_LONG]"
  {$NODE_NAMEPOINTCOUNT_GWY == 0} "[AP_RMP_TAXI_SPOT_SHORT]"
  #STATE(AL_RPGD_TAXI_LOOP_JUDGE)

<TW_RPGD_GWY_PASS:Gateway cross:ゲートウェイ通行できます>
